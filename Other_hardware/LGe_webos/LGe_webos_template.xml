                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>LG input select</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.3.2.0</snmp_oid>
                    <key>LG.inputselect</key>
                    <history>20d</history>
                    <trends>60d</trends>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>LGe webos</name>
                        </application>
                    </applications>
                    <valuemap>
