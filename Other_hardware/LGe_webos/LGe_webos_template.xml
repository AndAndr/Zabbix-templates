<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2023-06-22T13:44:19Z</date>
    <groups>
        <group>
            <name>Templates/Other Hardware</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template_LGe_webos</template>
            <name>Template_LGe_webos</name>
            <description>https://github.com/AndAndr/Zabbix-templates</description>
            <templates>
                <template>
                    <name>Template Module Generic SNMPv2</name>
                </template>
            </templates>
            <groups>
                <group>
                    <name>Templates/Other Hardware</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>LGe webos</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>LG aspect ratio</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.3.3.0</snmp_oid>
                    <key>LG.aspectratio</key>
                    <history>20d</history>
                    <trends>60d</trends>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>LGe webos</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>LG aspect ratio</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>HEX_TO_DECIMAL</type>
                            <params/>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1h</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>Check Aspect Ratio on {HOST.NAME}</name>
                            <opdata>Aspect Ratio: {ITEM.LASTVALUE1}</opdata>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>LG CPU temperature</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.3.24.0</snmp_oid>
                    <key>LG.CPUtemperature</key>
                    <history>20d</history>
                    <trends>60d</trends>
                    <units>`C</units>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>LGe webos</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>HEX_TO_DECIMAL</type>
                            <params/>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1h</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{avg(5m)}&gt;{$LGCPUMAXTEMP}</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{avg(5m)}&lt;{$LGCPUMAXTEMP}-2</recovery_expression>
                            <name>CPU temperature is too high on {HOST.NAME}</name>
                            <opdata>CPUTemperature: {ITEM.LASTVALUE1}</opdata>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>LG CPU Usage percent</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.2.9.0</snmp_oid>
                    <key>LG.CPUusage</key>
                    <history>20d</history>
                    <trends>60d</trends>
                    <units>%</units>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>LGe webos</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>^CPU\D+(\d+)%$
\1</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{avg(5m)}&gt;{$LGCPUMAXUTIL}</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{avg(5m)}&lt;{$LGCPUMAXUTIL}-10</recovery_expression>
                            <name>High CPU usage on {HOST.NAME}</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>LG Energy Saving</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.3.19.0</snmp_oid>
                    <key>LG.energysaving</key>
                    <history>20d</history>
                    <trends>60d</trends>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>LGe webos</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>LG energy saving</name>
                    </valuemap>
