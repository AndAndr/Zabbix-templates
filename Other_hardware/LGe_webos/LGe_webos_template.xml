                            <name>Check input on {HOST.NAME}</name>
                            <opdata>{ITEM.LASTVALUE1}</opdata>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>LG Memory usage percent</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.2.10.0</snmp_oid>
                    <key>LG.MEMusage</key>
                    <history>20d</history>
                    <trends>60d</trends>
                    <units>%</units>
                    <port>{$SNMP_PORT}</port>
                    <description>Что такое? Пока не понял.</description>
                    <applications>
                        <application>
                            <name>LGe webos</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>^.+\((\d+)%\)
\1</params>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1h</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>LG power state</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.7824.300.3.1.0</snmp_oid>
