                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.89.53.15.1.2.0</snmp_oid>
                    <key>rlPhdUnitEnvParamMainPSStatus</key>
