<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2024-03-25T10:44:11Z</date>
    <groups>
        <group>
            <name>Templates/Network devices</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Eltex ESR SNMPv2</template>
            <name>Template Eltex ESR SNMPv2</name>
            <description>https://github.com/AndAndr&#13;
Мониторинг по SNMPv2 маршрутизаторов Eltex ESR.&#13;
Тестировалось на ESR1000.&#13;
&#13;
Добавьте в зависимость к этому шаблону или подключите отдельно к хосту шаблон &quot;Template Module Generic SNMPv2&quot; для ZabbixV=4 или &quot;Generic by SNMP&quot; для ZabbixV&gt;=5</description>
            <groups>
                <group>
                    <name>Templates/Network devices</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Eltex chassis</name>
                </application>
                <application>
                    <name>Network interfaces</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>ESR Hardware Version</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.2.1.47.1.1.1.1.8.680000</snmp_oid>
                    <key>esrHardwareVersion</key>
                    <delay>3h</delay>
                    <history>14d</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <port>{$SNMP_PORT}</port>
                    <inventory_link>HARDWARE</inventory_link>
                    <applications>
                        <application>
                            <name>Eltex chassis</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Memory Free</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>1.3.6.1.4.1.9.9.48.1.1.1.6.1</snmp_oid>
                    <key>esrMemoryFree</key>
                    <history>14d</history>
                    <trends>60d</trends>
                    <units>B</units>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>Eltex chassis</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Memory Free %</name>
                    <type>CALCULATED</type>
                    <key>esrMemoryFreePercent</key>
                    <history>14d</history>
                    <trends>0</trends>
                    <units>%</units>
                    <params>100*last(esrMemoryFree)/last(esrMemoryTotal)</params>
                    <applications>
                        <application>
                            <name>Eltex chassis</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1h</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;{$MEM.FREE.CRIT}</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{last()}&gt;{$MEM.FREE.CRIT}+5</recovery_expression>
                            <name>Free memory low on {HOST.NAME}</name>
                            <opdata>Free mem percent: {ITEM.LASTVALUE1}</opdata>
                            <priority>AVERAGE</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Memory Total</name>
                    <type>CALCULATED</type>
                    <key>esrMemoryTotal</key>
                    <history>14d</history>
                    <trends>0</trends>
                    <units>B</units>
                    <params>last(esrMemoryUsed) + last(esrMemoryFree)</params>
                    <applications>
                        <application>
                            <name>Eltex chassis</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Memory Used</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>1.3.6.1.4.1.9.9.48.1.1.1.5.1</snmp_oid>
                    <key>esrMemoryUsed</key>
                    <history>14d</history>
                    <trends>60d</trends>
                    <units>B</units>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>Eltex chassis</name>
